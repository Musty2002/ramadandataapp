{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "ramadandataapp.com.ng"
        }
      ],
      "destination": "/website",
      "permanent": false
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "www.ramadandataapp.com.ng"
        }
      ],
      "destination": "/website",
      "permanent": false
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "admin.ramadandataapp.com.ng"
        }
      ],
      "destination": "/admin/login",
      "permanent": false
    },
    {
      "source": "/:path((?!admin|assets|_next).*)",
      "has": [
        {
          "type": "host",
          "value": "admin.ramadandataapp.com.ng"
        }
      ],
      "destination": "/admin/login",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
